<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:imo='urn:import:org.imogene.web.client.ui.field'
	xmlns:imo2='urn:import:org.imogene.web.client.ui.field.error'>

	<ui:style>

		.imogene-FormMultiEnum {
			margin: 0px;
			width: 200px;
			min-height:	20px;
			vertical-align: middle;
		}
		
		.imogene-Radio {
			vertical-align: top;
			margin-right: 10px;
		}
		
		.imogene-Radio input[type="radio"] {
			vertical-align: bottom;
			margin: 0px 3px 0px 0px;
		}
		@external .imogene-Radio;		
	</ui:style>


	<imo:ImogFieldAbstract ui:field="fieldBox">
		
		<g:VerticalPanel>

			<imo2:ImogErrorLabel ui:field="errorLabel" />
			
			<g:VerticalPanel ui:field="layout" stylePrimaryName="{style.imogene-FormMultiEnum}"/>			

		</g:VerticalPanel>

	</imo:ImogFieldAbstract>






</ui:UiBinder>