<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:imo='urn:import:org.imogene.web.client.ui.field'
	xmlns:imo1='urn:import:org.imogene.web.client.ui.field.relation.single'
	xmlns:imo2='urn:import:org.imogene.web.client.ui.field.relation.multi'
	xmlns:imo3="urn:import:org.imogene.web.client.ui.field.group"
	xmlns:imo4="urn:import:org.imogene.web.client.ui.field.binary"
	>

	<ui:style>
		.imogene-form {
			width: 100%;
			vertical-align: top;
			border-top: 1px solid #777;
			padding-top: 2px;
			margin-bottom: 2px;	
		}
		.imog-clearImage {
			cursor: pointer;
			cursor: hand;
			text-align: left;
			margin-left: 3px;
		}	
	</ui:style>


	<g:HorizontalPanel stylePrimaryName="{style.imogene-form}" verticalAlignment="middle">
	
		<g:cell verticalAlignment="middle" >
			<imo1:ImogSingleRelationBox ui:field="cardEntity" />
		</g:cell>
		<g:cell verticalAlignment="middle">
			<imo1:ImogSingleRelationBox ui:field="fieldGroup" />
		</g:cell>
		<g:cell verticalAlignment="middle" width="80px" horizontalAlignment="center">
			<imo:ImogCheckBox ui:field="read" />
		</g:cell>
		<g:cell verticalAlignment="middle" width="80px" horizontalAlignment="center">
			<imo:ImogCheckBox ui:field="write" />
		</g:cell>
		<g:cell verticalAlignment="middle" width="80px" horizontalAlignment="center">
			<imo:ImogCheckBox ui:field="export" />
		</g:cell>
	
	</g:HorizontalPanel>
	

</ui:UiBinder>
