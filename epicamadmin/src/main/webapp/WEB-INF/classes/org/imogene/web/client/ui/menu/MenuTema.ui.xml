		.imogene-MenuTemaContent {
			width: 100%;
			vertical-align: top;
			border-left: 2px solid #9fd2e7;
			padding: 4px 0px 4px 8px;
			margin-left: 3px;
		}
		
		.imogene-MenuTemaContent a {
			text-decoration: underline;
		}
