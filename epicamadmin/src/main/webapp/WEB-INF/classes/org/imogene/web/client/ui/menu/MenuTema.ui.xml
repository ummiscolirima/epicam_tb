	<g:DisclosurePanel ui:field="container"
		animationEnabled="true" stylePrimaryName="{style.imogene-MenuTema}">

		<g:customHeader>
			<g:Label ui:field="titleLabel" stylePrimaryName="{style.imogene-MenuTema-header}"></g:Label>
