            <method name="beforeOpen"><![CDATA[			
			count = 0;
			
			// Get instance of Renderer
			beanRenderer = Packages.org.imogene.epicam.server.EpicamServerRenderer.get();
			beanRenderer.setLocale(params["loc"]);
			
			
			reportServiceHandler = spring.getBean("reportHandler");
			
			beans = reportServiceHandler.getTreatmentForm("modified", false, params["patient"], params["trimestre"], params["annee"], 
			params["region"], params["district"], params["cdt"], params["personnel"], params["laboratoire"], params["dateDebut"], params["dateFin"]);	


]]></method>
            <property name="dataSource">ReportHandler</property>
            <method name="fetch"><![CDATA[if(beans !=null){
	if(count <beans.size()){
		var bean = beans.get(count);
		row["columnNom"] = bean.getColumnNom();
		row["columnSexe"] = bean.getColumnSexe();
		row["columnAdresse"] = bean.getColumnAdresse();
		count++;
		return true;
	}
}
return false;
]]></method>
